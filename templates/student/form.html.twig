<!DOCTYPE html>
<html>
<head>
    <title>Create Student</title>
</head>
<body>
    <h1>Create Student</h1>
    <form method="post" action="{{ path('create_student') }}">
        {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.email) }}
        {{ form_row(form.age) }}
        {{ form_row(form.class) }}
        {{ form_row(form._token) }}
        <button type="submit">Create</button>
        {{ form_end(form) }}
    </form>
</body>
</html>
